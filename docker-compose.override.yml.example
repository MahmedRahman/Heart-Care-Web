version: '3.8'

# Copy this file to docker-compose.override.yml and customize as needed
# This file is gitignored and allows you to override settings locally

services:
  app:
    environment:
      - APP_ENV=local
      - APP_DEBUG=true
      - APP_URL=http://localhost:8000

  nginx:
    ports:
      - "8000:80"

  mysql:
    ports:
      - "3306:3306"

